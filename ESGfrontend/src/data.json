{
  "modules": [
    {
      "name": "Workforce",
      "route": "/workforce",
      "icon": "Users",
      "submodules": [
        {
          "name": "Workforce Details",
          "questions": [
            {
              "name": "Employees and Workers Details",
              "type": "table",
              "content": {
                "title": "Employees",
                "fields": [
                  { "label": "Category", "key": "category", "align": "left" },
                  { "label": "Male No.", "key": "male", "align": "center" },
                  { "label": "Female No.", "key": "female", "align": "center" }
                ],
                "rows": [
                  {
                    "category": "Permanent",
                    "male": { "value": 0, "type": "input", "inputType": "number" },
                    "female": { "value": 0, "type": "input", "inputType": "number" }
                  },
                  {
                    "category": "Other than Permanent",
                    "male": { "value": 0, "type": "input", "inputType": "number" },
                    "female": { "value": 0, "type": "input", "inputType": "number" }
                  },
                  {
                    "category": "Total Employees",
                    "male": { "value": 0, "type": "calc", "formula": "rows[0].male.value / rows[1].male.value" },
                    "female": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.female?.value || 0), 0)" }
                  }
                ],
                "onEditClick": true
              }
            },
            {
              "name": "Differently Abled Employees and Workers",
              "type": "table",
              "content": {
                "title": "Differently Abled Employees",
