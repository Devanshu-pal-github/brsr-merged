                    "male2": { "value": 0, "type": "input", "inputType": "number" },
                    "female2": { "value": 0, "type": "input", "inputType": "number" }
                  },
                  {
                    "category": "Average",
                    "male1": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.male1?.value || 0), 0) / (rows.length - 1)" },
                    "female1": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.female1?.value || 0), 0) / (rows.length - 1)" },
                    "male2": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.male2?.value || 0), 0) / (rows.length - 1)" },
                    "female2": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.female2?.value || 0), 0) / (rows.length - 1)" }
