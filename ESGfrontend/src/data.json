                  {
                    "category": "Total",
                    "male": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.male?.value || 0), 0)" },
                    "female": { "value": 0, "type": "calc", "formula": "rows.slice(0, -1).reduce((sum, row) => sum + (row.female?.value || 0), 0)" }
